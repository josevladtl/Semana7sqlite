<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Semana7sqlite.Login">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Ventana de Login"/>

            <Entry x:Name="txtUsuario" Placeholder="Ingrese Usuario"/>
            <Entry x:Name="txtContrasena" Placeholder="Ingrese Contrasena"/>
            <Button x:Name="btnIniciar" Text="Iniciar Sesion" Clicked="btnIniciar_Clicked"/>
            <Button x:Name="btnRegistrar" Text="Registrar" Clicked="btnRegistrar_Clicked"/>
            

        </StackLayout>
    </ContentPage.Content>
</ContentPage>